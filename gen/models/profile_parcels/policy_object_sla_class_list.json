                      "oneOf": [
                        {
                          "properties": {
                            "optionType": {
                              "type": "string",
                              "enum": [
                                "global"
                              ]
                            },
                            "value": {
                              "default": "low",
                              "enum": [
                                "jitter",
                                "latency",
                                "loss",
                                "loss-latency",
                                "loss-jitter",
                                "latency-loss",
                                "latency-jitter",
                                "jitter-latency",
                                "jitter-loss",
                                "loss-latency-jitter",
                                "loss-jitter-latency",
                                "latency-loss-jitter",
                                "latency-jitter-loss",
